{
	"name": "fromholdio/silverstripe-csshelpers",
	"type": "silverstripe-vendormodule",
	"description": "Provides css hooks to add to your template BODY element, generated from user state and current page meta information.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "css"],
	"homepage": "https://github.com/fromholdio/silverstripe-csshelpers",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-csshelpers/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
	"require": {
		"php": ">=5.6",
		"silverstripe/vendor-plugin": "^1.0",
		"silverstripe/framework": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"Fromholdio\\CSSHelpers\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
